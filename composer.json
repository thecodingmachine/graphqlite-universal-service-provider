{
  "name": "thecodingmachine/graphqlite-universal-service-provider",
  "description": "Cross-framework module for GraphQLite using container-interop/service-provider",
  "license": "MIT",
  "type": "library",
  "autoload": {
    "psr-4": {
      "TheCodingMachine\\": "src/"
    }
  },
  "require": {
    "php": ">=7.1",
    "thecodingmachine/funky": "^1",
    "thecodingmachine/graphqlite": "^3",
    "thecodingmachine/doctrine-annotations-universal-module": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3.1",
    "phpstan/phpstan": "^0.10.3",
    "thecodingmachine/phpstan-strict-rules": "^0.10.3",
    "satooshi/php-coveralls": "^1.0.1",
    "mnapoli/simplex": "^0.5",
    "squizlabs/php_codesniffer": "^3.3.1",
    "thecodingmachine/symfony-cache-universal-module": "^1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -c phpstan.neon --level=7 --no-progress -vvv",
    "cs-check": "phpcs",
    "cs-fix": "phpcbf"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "suggest": {
    "thecodingmachine/symfony-cache-universal-module": "To provide a PSR-16 cache"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
